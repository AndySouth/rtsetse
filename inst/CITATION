year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)


bibentry(bibtype = "Manual",
         title = "{rtsetse}: for simulating tsetse populations",
         author = c(person("Andy", "South"),
                    person("Glyn", "Vale")),
         year = year,
         note = note,
         url = "https://github.com/AndySouth/rtsetse" )